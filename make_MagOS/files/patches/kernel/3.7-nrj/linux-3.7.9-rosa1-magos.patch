+sub config_name {
+    my ($cpu, $nproc, $mem, $name) = @_;
+    dprint "creating config name $cpu $nproc $mem $name\n";
+
+    if ( $name eq 'default' ) {
+	return "";
+    } elsif ($name eq 'none') {
+	@options = ($cpu, $nproc, $mem);
+	return "-" . join('-',grep(!/^none$/, @options));
+    } else {
+	return "-$name";
+    }
+}
